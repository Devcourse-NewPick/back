{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:mongo": "ts-node scripts/test-mongo.ts",
    "test:mysql": "ts-node scripts/test-mysql.ts",
    "prisma:generate:mysql": "prisma generate --schema=prisma/mysql.schema.prisma",
    "prisma:push:mysql": "prisma migrate dev --schema=prisma/mysql.schema.prisma",
    "generate:user": "ts-node scripts/generate-user.ts",
    "generate:subscriber": "ts-node scripts/generate-subscriber.ts",
    "generate:category": "ts-node scripts/generate-category.ts",
    "generate:all": "npm run generate:user && npm run generate:subscriber && npm run generate:category",
    "reset": "ts-node scripts/reset.ts",
    "prisma:studio": "prisma studio --schema=prisma/mysql.schema.prisma"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.0",
    "@esm2cjs/p-queue": "^7.3.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/elasticsearch": "^10.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@prisma/client": "^6.2.0",
    "@redis/client": "^1.6.0",
    "@sentry/nestjs": "^8.51.0",
    "@sentry/profiling-node": "^8.51.0",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis": "^2.1.0",
